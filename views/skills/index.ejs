<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Skills</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(s => { %>
      <li>
        <a href="/skills/<%= s.id %>"><%= s.name %></a>
        - <%= s.completed ? 'completed': 'not completed' %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
